{
  "expo": {
    "name": "salon16",
    "slug": "salon16",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "salon16",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.salon16.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Salon16 - Beauty Salon Management",
      "shortName": "Salon16",
      "description": "Professional salon management app",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#6C2A52",
      "backgroundColor": "#ffffff",
      "lang": "en",
      "scope": "/",
      "categories": ["business", "productivity"]
    },
    "extra": {
      "eas": {
        "projectId": "salon16"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "web": {
            "serviceWorker": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
